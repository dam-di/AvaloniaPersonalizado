<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AvaloniaPersonalizado.App"
             xmlns:local="using:AvaloniaPersonalizado"
             xmlns:views="clr-namespace:AvaloniaPersonalizado.Views"
             xmlns:vm="clr-namespace:AvaloniaPersonalizado.ViewModels"
             xmlns:styling="clr-namespace:FluentAvalonia.Styling;assembly=FluentAvalonia"
             RequestedThemeVariant="Default">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.DataTemplates>
        <local:ViewLocator/>
        <DataTemplate DataType="{x:Type vm:InicioViewModel}">
            <views:InicioView/>
        </DataTemplate>
        <DataTemplate DataType="{x:Type vm:ConfiguracionViewModel}">
            <views:ConfiguracionView/>
        </DataTemplate>
    </Application.DataTemplates>
  
    <Application.Styles>
        <styling:FluentAvaloniaTheme />
        <FluentTheme />
    </Application.Styles>
</Application>